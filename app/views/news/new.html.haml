=render 'common/project_tabs'
.row
  .span12
    #news.well
      = simple_form_for([@project, @news], :html => { :class => "form-horizontal", :multipart => true }) do |f|
        = render :partial => 'news/form', :locals => { :f => f }
        .form-actions
          = f.button :submit, "Create", :class => "btn-success"
