- model_class = Supervisor.new.class
= link_to "New #{model_class.model_name.human}", new_admin_supervisor_path, :class => "btn btn-success"
%h3=t '.title', :default => model_class.model_name.human.pluralize
/= link_to t('.new', :default => t("helpers.links.new")), new_admin_user_path, :class => 'btn btn-success'
%table.table.table-striped
  %thead
    %tr
      %th= model_class.human_attribute_name(:id)
      %th= model_class.human_attribute_name(:user)
      %th= model_class.human_attribute_name(:created_at)
      %th=t '.actions', :default => t("helpers.actions")
  %tbody
    - @supervisors.each do |supervisor|
      %tr
        %td= link_to supervisor.id, admin_supervisor_path(supervisor)
        %td= supervisor.user
        %td=l supervisor.created_at
        %td
          = link_to t('.edit', :default => t("helpers.links.edit")), edit_admin_user_path(supervisor.user), :class => 'btn btn-mini'
          = link_to t('.destroy', :default => t("helpers.links.destroy")), admin_supervisor_path(supervisor), :method => :delete, :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')), :class => 'btn btn-mini btn-danger'
