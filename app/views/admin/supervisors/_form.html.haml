= simple_form_for [:admin, @supervisor], :html => { :class => 'form-horizontal' } do |f|
  = f.association :user, :collection => User.not_admins
  .form-actions
    = f.button :submit, :class => 'btn-primary'
    = link_to t('.cancel', :default => t("helpers.links.cancel")), admin_supervisors_path, :class => 'btn'
