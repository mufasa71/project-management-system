=render 'common/project_tabs'
.row
  .span11
    = simple_form_for([@project, @document], :html => {:class => "form-vertical"}) do |f|
      = render :partial => "form", :locals => { :f => f }
      = link_to "Back", project_documents_path(@project)
      = f.button :submit, "Update", :class => "btn btn-success"
